.post-toc{
    border-radius: .3rem;
    width 15vh
    position fixed
    bottom 15vh
    right 0;
    z-index 9999
    background-color: var(--bg-t)
    box-shadow: var(--shadow)
    ol {
        list-style: none;
        padding-inline-start: 1rem;
    }
    .toc-list-item a{
        color: var(--second-text-color)
    }
    .is-active-li a{
        color: var(--default-link-color)
    }
    .tocbot-is-collapsed li{
        display none
    }
}
.post-toc{
    display: none
}
.display-inline{
    display inline!important
}
    .post-toc{
        width: 250px;
    }

@media (max-width 300px){
    .post-toc{
        max-width: 95vw;
    }
}

